<?xml version="1.0" encoding="UTF-8"?>
<part>
	<Author>FreeBee (Roland Bal)</Author>
	<Description>
		<en>Remote Controlled Decoupling</en>
		<fr>Découplage à distance</fr>
	</Description>
	<ImageURL>http://media.peeron.com/pics/inv/setpics/7862-1.1105127517.thumb2.jpg</ImageURL>
	<OldNameList>
	    <OldName>7862.7</OldName>
	</OldNameList>
	<SortingKey>12V new - E3</SortingKey>
	
	<!-- The margin of this part in stud for snapping on the grid, relative to the bitmap -->
	<SnapMargin>
		<left>0</left>
		<right>0</right>
		<top>7</top>
		<bottom>0</bottom>
	</SnapMargin>

	<!-- list of connection points (assuming that this part can connect to another one), the position of the connection is in stud coords and relative to the origin defined above) -->
	<ConnexionList>
		<connexion>
			<!-- type of connexion (0=none, 1=rail, 2=road, 3=monorail standard, 4=monorail short curve) -->
			<type>1</type>
			<position>
				<x>-8</x>
				<y>3.5</y>
			</position>
			<angle>180</angle>
			<angleToPrev>180</angleToPrev> <!-- how much angle between this connection and the prev one in this circular list -->
			<angleToNext>180</angleToNext> <!-- how much angle between this connection and the next one in this circular list -->			
			<nextConnexionPreference>1</nextConnexionPreference> <!-- index (0-based) in that list for connecting the next part -->
			<electricPlug>1</electricPlug>
		</connexion>
		<connexion>
			<!-- type of connexion (0=none, 1=rail, 2=road, 3=monorail standard, 4=monorail short curve) -->
			<type>1</type>
			<position>
				<x>8</x>
				<y>3.5</y>
			</position>
			<angle>0</angle>
			<angleToPrev>180</angleToPrev> <!-- how much angle between this connection and the prev one in this circular list -->
			<angleToNext>180</angleToNext> <!-- how much angle between this connection and the next one in this circular list -->			
			<nextConnexionPreference>0</nextConnexionPreference> <!-- index (0-based) in that list for connecting the next part -->
			<electricPlug>-1</electricPlug>
		</connexion>
	</ConnexionList>
	
	<hull>
		<point>
			<x>0</x>
			<y>56</y>
		</point>
		<point>
			<x>15</x>
			<y>56</y>
		</point>
		<point>
			<x>15</x>
			<y>0</y>
		</point>
		<point>
			<x>79</x>
			<y>0</y>
		</point>
		<point>
			<x>79</x>
			<y>56</y>
		</point>
		<point>
			<x>127</x>
			<y>56</y>
		</point>
		<point>
			<x>127</x>
			<y>119</y>
		</point>
		<point>
			<x>0</x>
			<y>119</y>
		</point>
	</hull>
	
	<!-- LDraw remapping data -->
	<LDraw>
           <PreferredHeight>-24</PreferredHeight>
           <SleeperID>767.8</SleeperID>
           <Translation>
              <x>0</x>
              <y>-70</y>
           </Translation>
	</LDraw>
</part>