<?xml version="1.0" encoding="UTF-8"?>
<part>
	<Author>FreeBee (Roland Bal), Alban Nanty</Author>
	<Description>
		<en>Train Track 9V Point Right Modified</en>
		<fr>Aiguillage droit 9V</fr>
	</Description>
	<ImageURL>http://media.peeron.com/pics/inv/custpics/2859.jpg</ImageURL>
	<SortingKey>09V - C3</SortingKey>
	
	<!-- The margin of this part in stud for snaping on the grid, relative to the bitmap -->
	<SnapMargin>
		<left>0.5</left>
		<right>0.5</right>
		<top>0</top>
		<bottom>6</bottom>
	</SnapMargin>
	<!-- list of connexion point (assuming that this part can connect to another one), the position of the connexion is in stud coord and relative to the origine defined above) -->
	<ConnexionList>
		<connexion>
			<!-- type of connexion (0=none, 1=rail, 2=road, 3=monorail standard, 4=monorail short curve) -->
			<type>1</type>
			<position>
				<x>-16</x>
				<y>-3</y>
			</position>
			<angle>180</angle>
			<angleToPrev>180.0</angleToPrev> <!-- how much angle between this connection and the prev one in this circular list -->
			<angleToNext>146.25</angleToNext> <!-- how much angle between this connection and the next one in this circular list -->			
			<nextConnexionPreference>2</nextConnexionPreference> <!-- index (0-based) in that list for connecting the next part -->
			<electricPlug>1</electricPlug>
		</connexion>
		<connexion>
			<!-- type of connexion (0=none, 1=rail, 2=road, 3=monorail standard, 4=monorail short curve) -->
			<type>1</type>
			<position>
				<x>6.22281</x>	<!-- x = connection0.X + radius * sin(33.75) = -16 + 40*sin(33.75) -->
				<y>3.7412155</y>	<!-- y = connection0.Y + radius * (1 - cos(33.75)) = -3 + 40*(1 - cos(33.75)) -->	
			</position>
			<angle>33.75</angle>
			<angleToPrev>146.25</angleToPrev> <!-- how much angle between this connection and the prev one in this circular list -->
			<angleToNext>33.75</angleToNext> <!-- how much angle between this connection and the next one in this circular list -->			
			<nextConnexionPreference>0</nextConnexionPreference> <!-- index (0-based) in that list for connecting the next part -->
			<electricPlug>-1</electricPlug>
		</connexion>
		<connexion>
			<!-- type of connexion (0=none, 1=rail, 2=road, 3=monorail standard, 4=monorail short curve) -->
			<type>1</type>
			<position>
				<x>16</x>	
				<y>-3</y>	
			</position>
			<angle>0</angle>
			<angleToPrev>33.75</angleToPrev> <!-- how much angle between this connection and the prev one in this circular list -->
			<angleToNext>180.0</angleToNext> <!-- how much angle between this connection and the next one in this circular list -->			
			<nextConnexionPreference>0</nextConnexionPreference> <!-- index (0-based) in that list for connecting the next part -->
			<electricPlug>-1</electricPlug>
		</connexion>
	</ConnexionList>
	<!-- a list of point that describe the hull of the parts to help finding the bounding box, when rotating the part. This is optionnal, if it doesn't exist we use the full box of the picture. -->
	<hull>
		<point>
			<x>0</x>
			<y>0</y>
		</point>
		<point>
			<x>263</x>
			<y>0</y>
		</point>
		<point>
			<x>263</x>
			<y>62</y>
		</point>
		<point>
			<x>199</x>
			<y>61</y>
		</point>
		<point>
			<x>160</x>
			<y>110</y>
		</point>
		<point>
			<x>43</x>
			<y>64</y>
		</point>
		<point>
			<x>43</x>
			<y>111</y>
		</point>
		<point>
			<x>17</x>
			<y>111</y>
		</point>
		<point>
			<x>17</x>
			<y>63</y>
		</point>
		<point>
			<x>0</x>
			<y>63</y>
		</point>
	</hull>
</part>