<?xml version="1.0" encoding="UTF-8"?>
<part>
	<Author>Alex McKenna</Author>
	<Description>
		<en>Baseplate 16 x 16 Driveway with Gray Drive with White Stripes Pattern</en>
		<fr>Plaque de base 16 x 16 avec une allée grise et des lignes blanches</fr>
	</Description>
	<ImageURL>http://media.peeron.com/ldraw/images/2/30225p02.png</ImageURL>

	<!-- list of connexion point (assuming that this part can connect to another one), the position of the connexion is in stud coord and relative to the center) -->	
	<ConnexionList>
		<connexion>
			<!-- type of connexion (0=none, 1=rail, 2=road, 3=monorail standard, 4=monorail short curve) -->
			<type>2</type>
			<position>
				<x>-8</x>
				<y>-3</y>
			</position>
			<angle>-90</angle>
			<angleToPrev>180</angleToPrev> <!-- how much angle between this connection and the prev one in this circular list -->
			<angleToNext>180</angleToNext> <!-- how much angle between this connection and the next one in this circular list -->			
			<nextConnexionPreference>1</nextConnexionPreference> <!-- index (0-based) in that list for connecting the next part -->
		</connexion>
		<connexion>
			<!-- type of connexion (0=none, 1=rail, 2=road, 3=monorail standard, 4=monorail short curve) -->
			<type>2</type>
			<position>
				<x>8</x>
				<y>-3</y>
			</position>
			<angle>90</angle>
			<angleToPrev>180</angleToPrev> <!-- how much angle between this connection and the prev one in this circular list -->
			<angleToNext>180</angleToNext> <!-- how much angle between this connection and the next one in this circular list -->			
			<nextConnexionPreference>0</nextConnexionPreference> <!-- index (0-based) in that list for connecting the next part -->
		</connexion>
	</ConnexionList>

	<!-- Track Designer remaping data -->
	<TrackDesigner>
		<ID>31003022</ID>
		<Flag>0</Flag>
		<HasSeveralGeometries>false</HasSeveralGeometries>
		<TDBitmapList>
    		<TDBitmap>
    			<BBConnexionPointIndex>0</BBConnexionPointIndex>
    			<Type>0</Type>
    			<AngleBetweenTDandBB>0</AngleBetweenTDandBB>
    		</TDBitmap>
		</TDBitmapList>
	</TrackDesigner>  
</part>